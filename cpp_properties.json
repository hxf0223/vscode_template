{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${default}",
                "/usr/local/llvm/include",
                "${workspaceFolder}/src",
                "${workspaceFolder}/include",
                "${workspaceFolder}/3rdparty/dmlc-core/include",
                "${workspaceFolder}/3rdparty/dlpack/include"
            ],
            "defines": [
                "TVM_LLVM_VERSION=90"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}